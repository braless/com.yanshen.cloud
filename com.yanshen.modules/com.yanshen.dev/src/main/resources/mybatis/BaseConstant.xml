<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yanshen.dev.mapper.TranscationDAO">
    <insert id="insertLog">
        insert into transcation_log (transcation_id, msg)
        values (#{transcationId}, #{msg})
    </insert>

    <update id="update">
        update weibo_url
        set id_card=32
        where id = 1
    </update>

    <update id="setBupdate">
        update weiBoUrl
        set age=2
        where id = 2
    </update>
</mapper>